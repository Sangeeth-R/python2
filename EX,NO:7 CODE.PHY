Program
import numpy as np
rows = int(input("Enter the number of rows: "))
cols = int(input("Enter the number of columns: "))
a = []
for i in range(rows):
r = []
for j in range(cols):
value = int(input(f’Enter value for element ({i+1},{j+1}): “))
r.append(value)
a.append(r)
a = np.array(a)
print("Original Array:")
print(a)
search = int(input("\nEnter the element to search for: "))
result = np.where(a = search)
if result[0].size > 0:
print(f"\nElement {search} found at position(s):")
for row, col in zip(result[0], result[1]):
print(f'Row: {row+1}, Column: \{col + 1\} ^ prime prime )
else:
print("\nElement {search} not found in the array.")
