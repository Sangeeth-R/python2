d = {}
n = int(input("Enter number of key-value pairs: "))
for i in range(n):
 key = input("Enter key: ")
 value = input("Enter value: ")
 d[key] = value
keycheck = input("Enter key to check: ")
print("Key exists" if keycheck in d else "Key does not exist")
valuecheck = input("Enter value to check: ")
print("Value exists" if valuecheck in d.values() else "Value does not exist")
